<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>cc.macloud</groupId>
	<artifactId>core</artifactId>
	<version>1.0.2-SNAPSHOT</version>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.10.1</version>
				<configuration>
					<release>11</release>
					<encoding>UTF-8</encoding>
					<verbose>true</verbose>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>2.1.2</version>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar-no-fork</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
		<resources>
			<resource>
				<directory>src/main/java</directory>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</resource>
		</resources>
		<testResources>
			<testResource>
				<directory>src/test/java</directory>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</testResource>
		</testResources>
	</build>
	<distributionManagement>
		<repository>
			<id>macloud.default</id>
			<name>Internal Release Repository</name>
			<url>https://repo.macloud.cc/archiva/repository/internal/</url>
		</repository>
		<snapshotRepository>
			<id>macloud.snapshots</id>
			<name>Internal Snapshot Repository</name>
			<url>https://repo.macloud.cc/archiva/repository/snapshots/</url>
		</snapshotRepository>
	</distributionManagement>
	<properties>
		<spring-version>5.3.18</spring-version>
		<spring-security-version>5.5.7</spring-security-version>
		<hibernate-core-version>5.4.24.Final</hibernate-core-version>
		<hibernate-annotations-version>5.1.0.Final</hibernate-annotations-version>
		<lucene-version>3.6.0</lucene-version>
		<slf4j-version>1.7.30</slf4j-version> <!-- 1.6.4 newer ;1.5.11 for spring -->
